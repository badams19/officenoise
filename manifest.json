{
  "name": "OfficeNoise",
  "version": "1.0",
  "description": "You guess the price, you win the prize.",
  "permissions": ["activeTab", "tabs"],
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "web_accessible_resources": [
    "noises/bearisbestpt1.ogg",
     "noises/bearisbestpt2.ogg",
     "background.js"
  ],
  "commands": {
    "choose-noise": {
      "suggested_key": {
        "default": "Ctrl+Shift+Y",
        "mac": "Command+Shift+Y"
      },
      "description": "Choose random office noise"
    },
    "_execute_browser_action": {
      "suggested_key": {
        "windows": "Ctrl+Shift+K",
        "mac": "Command+Shift+K"
      }
    }
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}